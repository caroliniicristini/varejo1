{
  "store.home": {
    "children": [
      "responsive-layout.mobile",
      "responsive-layout.desktop"
    ]
  },
  "responsive-layout.desktop": {
    "children": [
      "list-context.image-list#demo",
      "flex-layout.row#deals",
      "rich-text#-title",
      "list-context.image-list#demo1",
      "rich-text#shelf-title",
      "flex-layout.row#shelf",
      "flex-layout.row#imgmob",
      "flex-layout.row#bannerdesk",
      "rich-text#text",
      "rich-text#shelf-ultop",
      "flex-layout.row#shelf",
      "rich-text#shelf-titleshelf3",
      "flex-layout.row#shelf",
      "newsletter",
      "__fold__.experimentalLazyAssets"
    ]
  },
  "responsive-layout.mobile": {
    "children": [
      "flex-layout.row#search-mobile",
      "list-context.image-list#demo",
      "flex-layout.row#deals",
      "rich-text#-title",
      "list-context.image-list#demo1",
      "rich-text#shelf-title",
      "flex-layout.row#shelf",
      "flex-layout.row#imgmob",
      "flex-layout.row#bannerdesk",
      "rich-text#text",
      "rich-text#shelf-ultop",
      "flex-layout.row#shelf",
      "rich-text#shelf-titleshelf3",
      "flex-layout.row#shelf",
      "newsletter",
      "__fold__.experimentalLazyAssets"
    ]
  },
  "newsletter": {
    "props": {
      "blockClass": "newsletter",
      "label": "Acompanhe nossas novidades.\n Cadastre-se em nossas newsletter.",
      "submit": "Cadastrar",
      "placeholder": "Digite seu e-mail"
    }
  },
  "shelf#home": {
    "blocks": [
      "product-summary.shelf"
    ]
  },
  "list-context.image-list#demo": {
    "children": [
      "slider-layout#demo-images"
    ],
    "props": {
      "blockClass": "imgprincipal",
      "height": 720,
      "images": [
        {
          "image": "assets/bannerPrincipal.png",
          "mobileImage": "assets/img1.png"
        },
        {
          "image": "assets/bannerPrincipal.png",
          "mobileImage": "assets/img1.png"
        },
        {
          "image": "assets/bannerPrincipal.png",
          "mobileImage": "assets/img1.png"
        }
      ]
    }
  },
  "slider-layout#demo-images": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    }
  },
  "list-context.image-list#demo1": {
    "children": [
      "slider-layout#demo-images2"
    ],
