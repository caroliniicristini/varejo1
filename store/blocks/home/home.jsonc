        },
        {
          "image": "assets/beleza.png",
          "mobileImage": "assets/beleza.png"
        },
        {
          "image": "assets/decor.png",
          "mobileImage": "assets/decor.png"
        },
        {
          "image": "assets/audio.png",
          "mobileImage": "assets/audio.png"
        }
      ]
    }
  },
  "slider-layout#demo-images2": {
    "props": {
      "itemsPerPage": {
        "desktop": 8,
        "tablet": 1,
        "phone": 2
