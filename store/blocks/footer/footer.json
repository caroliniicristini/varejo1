{
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
    ]
  },
  "footer-layout.desktop": {
    "children": [
      "newsletter",
      "flex-layout.row#footer-2-desktop",

      "flex-layout.row#4-footer-mobile",
      
      "rich-text#textfooter",
      "flex-layout.row#5-footer-desk"
      
    ]
  },
  "flex-layout.row#footer-1-desktop": {
    "children": [
      "vtex.menu@2.x:menu#footer-sobre",
      "vtex.menu@2.x:menu#footer-atendimento-mobile",
      "vtex.menu@2.x:menu#footer-ajuda-mobile",
      "flex-layout.row#3-footer-mobile",
      "social-networks"
    ],
    "props": {
      "blockClass": "menu-row",
      "paddingTop": 6,
      "paddingBottom": 3,
      "colSpacing": "auto"
    }
  },
  "social-networks": {
    "props": {
      "socialNetworks": [
        {
          "name": "Facebook",
          "src": "assets/facebook.png"
        },
        {
          "name": "Instagram",
          "src": "assets/instagram.png"
        },
        {
          "name": "Youtube",
          "src": "assets/youtube.png"
        }
      ]
    }
  },
  "footer-layout.mobile": {
    "children": [
      "flex-layout.row#1-footer-mobile",
      "flex-layout.row#2-footer-mobile",
      "flex-layout.row#3-footer-mobile",
      "flex-layout.row#4-footer-mobile",
      "rich-text#textfooter",
      "flex-layout.row#5-footer-mobile"
    ]
  },
  "flex-layout.row#2-footer-mobile": {
    "children": [
      "flex-layout.col#footer-1-mobile"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.col#footer-1-mobile": {
    "children": [
      "accepted-payment-methods",
      "social-networks"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },
  "flex-layout.row#1-footer-mobile": {
    "props": {
      "paddingTop": 4,
      "paddingBottom": 4
    },
    "children": [
      "vtex.menu@2.x:menu#footer-mobile"
    ]
  },
  "flex-layout.row#3-footer-mobile": {
    "children": [
      "image#footer1",
      "image#footer2",
      "image#footer3",
      "image#footer4",
      "image#footer5",
      "image#footer6"
    ],
    "props": {
    
      "blockClass": "imgsfooter",
      "preserveLayoutOnMobile": true
    }
  },
  "flex-layout.row#4-footer-mobile": {
    "children": [
      "image#footer7",
      "image#footer8",
      "image#footer9"
    ],
    "props": {
 
      "blockClass": "imgsfooter1",
      "preserveLayoutOnMobile": true
    }
  },
  "image#footer1": {
    "props": {
      "src": "assets/visa.png",
      "blockClass": "footer"
    }
  },
  "image#footer2": {
    "props": {
      "src": "assets/master.png",
      "blockClass": "footer"
    }
  },
  "image#footer3": {
    "props": {
      "src": "assets/diners.png",
      "blockClass": "footer"
    }
  },
  "image#footer4": {
    "props": {
      "src": "assets/amex.png",
      "blockClass": "footer"
    }
  },
  "image#footer5": {
    "props": {
      "src": "assets/elo.png",
      "blockClass": "footer"
    }
  },
  "image#footer6": {
    "props": {
      "src": "assets/boleto.png",
      "blockClass": "footer"
    }
  },
  "image#footer7": {
    "props": {
      "src": "assets/letsencrypt.png",
      "blockClass": "lets"
    }
  },
  "image#footer8": {
    "props": {
      "src": "assets/ebit.png",
      "blockClass": "ebit"
    }
  },
  "image#footer9": {
    "props": {
      "src": "assets/pci.png",
      "blockClass": "cert"
    }
  },
  "flex-layout.row#5-footer-mobile": {
    "children": [
      "image#footer12",
      "image#footer11"
    ],
    "props": {
     
      "blockClass": "imgsfooter",
      "preserveLayoutOnMobile": true
    }
  },
  "flex-layout.row#5-footer-desk": {
    "children": [
      "image#footer10",
      "image#footer11"
    ],
    "props": {
      
      "blockClass": "imgsfooter",
      "preserveLayoutOnMobile": true
    }
  },
  "image#footer10": {
    "props": {
      "src": "assets/avanti1.png",
      "blockClass": "avanti",
      "href":"https://penseavanti.com.br/"
    }
  },
  "image#footer12": {
    "props": {
      "src": "assets/avanti2.png",
      "blockClass": "avanti2",
      "href":"https://penseavanti.com.br/"
    }
  },
  "image#footer11": {
    "props": {
      "src": "assets/vtex.png",
      "blockClass": "vtex",
      "href": "https://vtex.com/br-pt/"
    }
  },
