    "props": {
      "blockClass": "textfooter",
      "text": "AVANTI S.A Â© 2020. TODOS OS DIREITOS RESERVADOS. "
    }
  },
  "flex-layout.row#footer-2-desktop": {
    "children": [
      "flex-layout.col#sobre2",
      "flex-layout.col#atendimento2",
      "flex-layout.col#ajuda",
      "flex-layout.col#pagamento",
      "flex-layout.col#redes"
    ],
    "props": {
      "blockClass": "alltitles",
      "colSizing": "auto",
      "fullWidth": false,
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true
    }
  },
  "flex-layout.col#sobre2": {
    "children": [
      "rich-text#sobre",
      "vtex.menu@2.x:menu#footer-sobre"
    ],
    "props": {
      "blockClass": "colSobre",
      "width": "auto",
      "horizontalAlign": "left",
      "verticalAlign": "top"
    }
  },
  "rich-text#sobre": {
    "props": {
      "text": "Sobre",
      "blockClass": "titulo"
    }
  },
  "rich-text#vazio": {
    "props": {
      "text": ""
    }
  },
  "flex-layout.col#atendimento2": {
    "children": [
      "rich-text#atendimento",
      "vtex.menu@2.x:menu#footer-atendimento-mobile"
    ],
    "props": {
      "blockClass": "colSobre",
      "width": "auto",
      "horizontalAlign": "left",
      "verticalAlign": "top"
    }
  },
  "flex-layout.col#menuAbout":{
    "children":[
      "flex-layout.col#sobre2",
      "flex-layout.col#atendimento2",
      "flex-layout.col#ajuda"
