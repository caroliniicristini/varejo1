      "vtex.menu@2.x:menu#footer-ajuda-mobile"
    ],
    "props": {
      "blockClass": "colSobre",
      "width": "auto",
      "horizontalAlign": "left",
      "verticalAlign": "top"
    }
  },
  "rich-text#ajuda": {
    "props": {
      "text": "Ajuda",
      "blockClass": "titulo"
    }
  },
  "flex-layout.col#pagamento": {
    "children": [
      "rich-text#pagamento-title",
      "rich-text#vazio",
      "rich-text#vazio",
      "image#visa",
      "image#master",
      "image#diners",
      "image#amex",
      "image#elo",
      "image#boleto"
    ],
    "props": {
      "blockClass": "colPagamento",
